	<subDataset name="BrandPreferences" resourceBundle="supermart" whenResourceMissingType="Key" uuid="584f5306-d383-462c-b7c9-0d43a2df9be8">
		<parameter name="custID" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     sum(sales_fact_1997.unit_sales)as unit_sales,
     product.brand_name AS product_brand_name
FROM
     sales_fact_1997 sales_fact_1997 INNER JOIN product product ON sales_fact_1997.product_id = product.product_id
WHERE
     sales_fact_1997.customer_id = $P{custID}
GROUP BY
     product.brand_name
ORDER BY
     unit_sales DESC]]>
		</queryString>
		<field name="unit_sales" class="java.math.BigDecimal"/>
		<field name="product_brand_name" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ShoppingDay" resourceBundle="supermart" whenResourceMissingType="Key" uuid="449dc8e9-3c70-48ba-ade9-c33283fbf86b">
		<parameter name="customerId" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     case
       when count_sales is null then 0
       else count_sales
     end               as count_sales,
     days.the_date     as the_date,
     days.the_day      as days_week_day, /* This column is returned only as quick validity check. It is not used in the report. */
     days.day_of_week
FROM
     ( select * from time_by_day where time_id between 367 and 373 ) days /* This inline view gets an arbitrary week so that all weekdays are represented */
     LEFT OUTER JOIN (
          SELECT
               count(distinct sales_fact_1997.time_id) as count_sales,
               time_by_day.day_of_week as time_by_day_day_of_week
          FROM
               sales_fact_1997 sales_fact_1997
               INNER JOIN time_by_day time_by_day ON sales_fact_1997.time_id = time_by_day.time_id
          WHERE
               sales_fact_1997.customer_id = $P{customerId}
          GROUP BY
               time_by_day.day_of_week
      ) sales_by_day_of_week on days.day_of_week = sales_by_day_of_week.time_by_day_day_of_week
ORDER BY
     days.day_of_week]]>
		</queryString>
		<field name="count_sales" class="java.lang.Long"/>
		<field name="days_week_day" class="java.lang.String"/>
		<variable name="SimpleDayOfWeek" class="java.text.SimpleDateFormat">
			<variableExpression><![CDATA[new SimpleDateFormat( "E", $P{REPORT_LOCALE} )]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="AvgSale" uuid="ea6e0cb9-a49d-46df-a26f-c2924e60810b">
		<parameter name="custID" class="java.lang.Integer" isForPrompting="false">
			<defaultValueExpression><![CDATA[2724]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     avg(agg_l_03_sales_fact_1997.store_sales) AS avg_ALL,
     null AS customer_id,
     'ALL CUSTOMERS' as customers
FROM
     agg_l_03_sales_fact_1997 agg_l_03_sales_fact_1997
union
SELECT
     avg(agg_l_03_sales_fact_1997.store_sales)AS avg_sales,
     customer.customer_id AS customer_id,
     upper(substring(customer.fname,1,1)) || '. ' || UPPER(customer.lname) AS customer_lname
FROM
     customer customer INNER JOIN agg_l_03_sales_fact_1997 agg_l_03_sales_fact_1997 ON customer.customer_id = agg_l_03_sales_fact_1997.customer_id
WHERE
     agg_l_03_sales_fact_1997.customer_id = $P{custID}
GROUP BY
     customer.customer_id,upper(substring(customer.fname,1,1)) || '. ' || UPPER(customer.lname)]]>
		</queryString>
		<field name="avg_all" class="java.math.BigDecimal"/>
		<field name="customer_id" class="java.lang.Integer"/>
		<field name="customers" class="java.lang.String"/>
	</subDataset>
