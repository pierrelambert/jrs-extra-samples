     customer.total_children AS customer_total_children,
     customer.num_children_at_home AS customer_num_children_at_home,
     customer.education AS customer_education,
     customer.date_accnt_opened AS customer_date_accnt_opened,
     customer.member_card AS customer_member_card,
     customer.occupation AS customer_occupation,
     customer.houseowner AS customer_houseowner,
     customer.num_cars_owned AS customer_num_cars_owned,
     upper(customer.fullname) AS customer_fullname
FROM
     customer customer INNER JOIN agg_l_03_sales_fact_1997 agg_l_03_sales_fact_1997 ON customer.customer_id = agg_l_03_sales_fact_1997.customer_id
WHERE
     agg_l_03_sales_fact_1997.customer_id = $P{customerId}
GROUP BY
     customer.customer_id,
     customer.account_num,
     customer.lname,
     customer.fname,
     customer.mi,
     customer.address1,
     customer.address2,
     customer.address3,
