		<parameter name="ProductFamily" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select r.productdep,r.totalsales,r.femalesales,r.malesales from
(
	SELECT 'All' as productdep,sum(store_sales) as totalsales,sum(case when gender='F' then store_sales end) as femalesales,sum(case when gender='M' then store_sales end) as malesales,1 as ord
	FROM sales_fact_1997 s inner join store st on s.store_id=st.store_id
	inner join customer c on c.customer_id=s.customer_id
	inner join product p on p.product_id=s.product_id
	inner join product_class pc on p.product_class_id=pc.product_class_id
	where $X{IN,pc.product_family,ProductFamily}
	group by productdep
	union
	SELECT pc.product_department as productdep,sum(store_sales) as totalsales,sum(case when gender='F' then store_sales end) as femalesales,sum(case when gender='M' then store_sales end) as malesales,2 as ord
	FROM sales_fact_1997 s inner join store st on s.store_id=st.store_id
	inner join customer c on c.customer_id=s.customer_id
	inner join product p on p.product_id=s.product_id
	inner join product_class pc on p.product_class_id=pc.product_class_id
	where $X{IN,pc.product_family,ProductFamily}
	group by pc.product_department
) r
group by r.productdep,r.totalsales,r.femalesales,r.malesales,r.ord
order by r.ord]]>
		</queryString>
		<field name="productdep" class="java.lang.String"/>
		<field name="totalsales" class="java.math.BigDecimal"/>
		<field name="femalesales" class="java.math.BigDecimal"/>
		<field name="malesales" class="java.math.BigDecimal"/>
	</subDataset>
