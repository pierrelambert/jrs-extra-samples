	<subDataset name="Table Dataset 1" uuid="a835a05b-bfcb-4f20-b004-8de76ce0c69a">
		<parameter name="customerId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA["6284"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     time_by_day.the_day          AS time_by_day_the_day,
     time_by_day.the_date         AS time_by_day_the_date,
     product.product_name         AS product_product_name,
     product.recyclable_package   AS product_recyclable_package,
     product.low_fat              AS product_low_fat,
     promotion.promotion_name     AS promotion_promotion_name,
     sales_fact_1997.store_sales  AS sales_fact_1997_store_sales,
     sales_fact_1997.unit_sales   AS sales_fact_1997_unit_sales,
     time_by_day.the_date
FROM
     sales_fact_1997 sales_fact_1997 INNER JOIN promotion promotion ON sales_fact_1997.promotion_id = promotion.promotion_id
     INNER JOIN product product ON sales_fact_1997.product_id = product.product_id
     INNER JOIN time_by_day time_by_day ON sales_fact_1997.time_id = time_by_day.time_id
WHERE
     sales_fact_1997.customer_id = $P{customerId}
ORDER BY
     time_by_day.the_date ASC]]>
		</queryString>
